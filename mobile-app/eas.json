{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "simulator": {
      "channel": "simulator",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production"
    }
  },
  "submit": {
    "development": {
      "android": {
        "changesNotSentForReview": true,
        "track": "internal",
        "releaseStatus": "completed"
      }
    },
    "production": {
      "android": {
        "changesNotSentForReview": false,
        "track": "production",
        "releaseStatus": "completed"
      },
      "ios": {
        "appleId": "Brian@nodeeight.org",
        "ascAppId": "1590818516",
        "appleTeamId": "Y3L8CUY432",
        "metadataPath": "./store.config.js"
      }
    }
  }
}
