{
  "cli": {
    "appVersionSource": "local"
  },
  "build": {
    "simulator": {
      "channel": "simulator",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production",
      "ios": {}
    }
  },
  "submit": {
    "development": {
      "android": {
        "changesNotSentForReview": true,
        "track": "internal",
        "releaseStatus": "completed"
      }
    },
    "production": {
      "android": {
        "track": "production",
        "releaseStatus": "completed"
      }
    }
  }
}
